type Receivable {
    id: ID!
    contract: Contract!
    customerName: String!
    amount: Float!
    currency: Currency!
    dueDate: DateTime
    status: ReceivableStatus!
    createdAt: DateTime!
    updatedAt: DateTime!
}

input ReceivableInput {
    customerName: String!
    amount: Float!
    currencyCode: String!
    status: ReceivableStatus!
}