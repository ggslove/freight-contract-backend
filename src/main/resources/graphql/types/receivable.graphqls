type Receivable {
  id: ID!
  contract: Contract!
  financeItem: String!
  amount: String!
  currency: Currency!
  dueDate: DateTime
  status: ContractStatus!
  createdAt: DateTime!
  updatedAt: DateTime!
}

input ReceivableInput {
  financeItem: String!
  amount: String!
  currencyCode: String!
  status: ContractStatus!
}