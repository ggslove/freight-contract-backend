type Payable {
    id: ID!
    contract: Contract!
    supplierName: String!
    amount: Float!
    currency: Currency!
    dueDate: DateTime
    status: PayableStatus!
    createdAt: DateTime!
    updatedAt: DateTime!
}

input PayableInput {
    supplierName: String!
    amount: Float!
    currencyCode: String!
    status: PayableStatus!
}